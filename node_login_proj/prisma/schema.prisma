generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int      @id @default(autoincrement())
  email      String              // ❌ NOT unique
  username   String   @unique    // ✅ UNIQUE
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
