generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model users {
  ID                   Int      @id @default(autoincrement())
  company_name         String   @db.VarChar(100)
  brand_name           String   @db.VarChar(100)
  owner_name           String   @db.VarChar(100)
  mobile               String   @db.VarChar(12)
  email                String   @db.VarChar(50)
  pan_number           String   @db.VarChar(12)
  aadhar_number        String?  @db.VarChar(120)
  documents            String?  @db.VarChar(200)
  address              String?  @db.Text
  account_number       String   @db.VarChar(25)
  ifsc_code            String   @db.VarChar(20)
  bank_name            String   @db.VarChar(50)
  value1               String   @default("1804") @db.VarChar(50)
  value2               String   @default("1805") @db.VarChar(50)
  value3               Float    @default(3) @db.Float
  value4               Float    @default(0) @db.Float
  value5               Float    @default(0)
  balance              Float
  commission           Float
  pin                  String   @default("7777") @db.VarChar(9)
  admin_id             Int
  block                Int      @default(0)
  status               Int      @default(0)
  password             String   @db.VarChar(100)
  type                 String   @db.VarChar(100)
  portal_charge        Float    @default(0) @db.Float
  aeps_plan_id         Int      @default(1806)
  api_token            String?  @db.VarChar(255)
  callback_url         String?  @db.Text
  upi_callback         String?  @db.Text
  upi_bal              Float?   @default(0)
  unique_id            String?  @db.VarChar(100)
  pincode              String?  @db.VarChar(7)
  cap_balance          Float?   @default(0) @db.Float
  payout_bal           Float?   @default(0)
  matm_bal             Float?   @default(0)
  key                  String?  @db.Text
  aeps_status          String   @default("false") @db.VarChar(20)
  pipe                 String   @default("bank1") @db.VarChar(50)
  date                 DateTime @default(now()) @db.DateTime(0)
  matm_plan_id         Int?     @default(1806)
  upi_transfer_plan_id Int?     @default(1806)
  upi_collect_plan_id  Int?     @default(1806)
  payout_plan_id       Int?     @default(1806)
  sale_force_id        String?  @db.VarChar(100)
  payout_pipe          String?  @db.VarChar(20)
  enforce_pipe         Int?     @default(0) @db.TinyInt

  @@map("users")
}

