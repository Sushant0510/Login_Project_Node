  cp     Float @db.Float
  eko    Float @db.Float
  o2pay  Float @db.Float

  @@index([ID], map: "idx_all_operator_balance_ID")
}

model api_logs {
  ID                Int     @id @default(autoincrement())
  type              String  @db.VarChar(30)
  request_id        String? @db.VarChar(255)
  request           String  @db.Text
  response          String  @db.Text
  date              String? @db.VarChar(20)
  time              String? @db.VarChar(20)
  status            String? @default("0") @db.VarChar(50)
  mobile            String? @db.VarChar(20)
  tid               String? @db.VarChar(45)
  delay             Float?  @db.Float
  action            String? @db.VarChar(50)
  stm_id            BigInt? @default(0)
  api_id            Int?
  txn_type          String? @db.VarChar(100)
  api_number        Int?
  provider          String? @db.VarChar(100)
  incoming_request  String? @db.Text
  incoming_response String? @db.Text

  @@unique([ID, date], map: "idx_unique_date")
  @@index([mobile], map: "api_logs_idx_mobile")
  @@index([tid], map: "api_logs_idx_tid")
  @@index([api_number, status], map: "idx_api_logs_api_number_status")
  @@index([date, time, status, mobile, tid, txn_type, type, request_id, provider, api_id, api_number, delay, stm_id], map: "idx_api_logs_composite_max")
  @@index([date, delay], map: "idx_api_logs_date_delay")
  @@index([date, provider, status], map: "idx_api_logs_date_provider_status")
  @@index([date, response(length: 555), status, type], map: "idx_api_logs_date_response_status_type")
  @@index([date, tid], map: "idx_api_logs_date_tid")
  @@index([date, tid, status], map: "idx_api_logs_date_tid_status")
  @@index([date, time, status], map: "idx_api_logs_date_time_status")
  @@index([mobile, status], map: "idx_api_logs_mobile_status")
  @@index([provider, type], map: "idx_api_logs_provider_type")
  @@index([request_id, api_id], map: "idx_api_logs_request_id_api_id")
  @@index([status, date, tid, provider, txn_type, mobile], map: "idx_api_logs_status_date_tid")
  @@index([date, time, delay, api_id, status, tid], map: "idx_api_logs_time_delay")
  @@index([txn_type, status], map: "idx_api_logs_txn_type_status")
  @@index([date], map: "idx_date")
  @@index([provider], map: "idx_provider")
  @@index([provider, tid, date], map: "idx_provider_tid_date")
  @@index([tid], map: "idx_tid")
}

